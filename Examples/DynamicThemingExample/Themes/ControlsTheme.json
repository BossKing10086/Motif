{
    ".Button": {
        "text": ".SubheadText",
        "contentEdgeInsets": "{10.0, 20.0, 10.0, 20.0}",
        "cornerRadius": 5.0
    },
    ".DestructiveButton": {
        "_superclass": ".Button",
        "backgroundColor": "$DestructiveColor",
        "tintColor": "$PrimaryButtonTextColor"
    },
    ".CreativeButton": {
        "_superclass": ".Button",
        "backgroundColor": "$PrimaryColor",
        "tintColor": "$PrimaryButtonTextColor"
    },
    ".SecondaryButton": {
        "_superclass": ".Button",
        "borderWidth": 1.0
    },
    ".DestructiveSecondaryButton": {
        "_superclass": ".SecondaryButton",
        "tintColor": "$DestructiveColor",
        "borderColor": "$DestructiveColor"
    },
    ".CreativeSecondaryButton": {
        "_superclass": ".SecondaryButton",
        "tintColor": "$PrimaryColor",
        "borderColor": "$PrimaryColor"
    },
    ".Toggle": {
        "onTintColor": "$PrimaryColor",
        "tintColor": "$SeparatorColor",
    },
    ".Slider": {
        "minimumTrackTintColor": "$PrimaryColor",
        "maximumTrackTintColor": "$SeparatorColor"
    },
    ".SegmentedControlText": {
        "_superclass": ".SubheadText",
        "color": "$PrimaryColor"
    },
    ".SegmentedControl": {
        "tintColor": "$PrimaryColor",
        "text": ".SegmentedControlText"
    }
}
